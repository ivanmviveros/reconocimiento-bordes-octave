Universe                = docker
docker_image            = simexp/octave
Executable              = ./runOctave.bash
arguments               = -qfW octave_runner.m rubik.png $(threshold)
should_transfer_files   = YES
transfer_input_files    = runOctave.bash, octave_runner.m,rubik.png,canny_sobel.m,
when_to_transfer_output = ON_EXIT_OR_EVICT
output                  = logs/out.$(threshold)
error                   = logs/err.$(threshold)
log                     = logs/log.$(threshold)
request_memory          = 1024M
queue 1
